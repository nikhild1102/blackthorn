    "db:migrate": "node_modules/.bin/sequelize db:migrate",
    "db:seed": "node_modules/.bin/sequelize db:seed:all",
    "start:dev": "nodemon --inspect=0.0.0.0:8521 ./bin/dev",
    "prestart": "npm run clean && npm run build-server",
    "start": "node build/bin/dev"
  },
  "devDependencies": {
    "artillery": "^1.6.0-28",
    "eslint": "^4.19.1",
