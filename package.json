    "nodemon": "^1.12.1",
    "standard": "^10.0.3"
  },
  "_moduleAliases": {
    "@controllers": "app/controllers",
    "@helpers": "app/helpers",
    "@models": "app/models",
    "@services": "app/services",
    "@server": "server"
